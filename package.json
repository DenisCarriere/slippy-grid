  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "prepublish": "npm run test && npm run rollup",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "lint": "standard",
