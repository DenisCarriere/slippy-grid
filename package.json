    "docs": "documentation readme index.js --section=API",
    "rollup": "rollup -c docs/rollup.config.js"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@turf/bbox": "^3.10.5",
    "@turf/bbox-polygon": "^3.10.5",
    "@turf/explode": "^3.10.5",
    "@turf/inside": "^3.10.5",
    "global-mercator": "^2.2.2"
  },
  "devDependencies": {
    "@turf/turf": "^3.11.2",
    "@types/geojson": "^1.0.0",
    "benchmark": "^2.1.3",
    "coveralls": "^2.11.16",
    "documentation": "^4.0.0-beta.18",
    "geojson-normalize": "^0.0.1",
    "jest-cli": "^18.1.0",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "standard": "^8.6.0",
    "typescript": "^2.1.6",
    "write-json-file": "^2.0.0"
